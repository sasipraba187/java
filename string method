package day9;
class Bird
{
	String color="purple";
	void eat()
	{
		System.out.println("Eating");
	}
	class dog extends Bird
	{
		void bark()
		{
			System.out.println("Barking");
		}
	void display()
	{
		super.eat();
		bark();
	}
	}
}
public class problem5 
{
	public static void main(String[] args) {
	dog obj=new dog();
	obj.display();
	}
}
	


