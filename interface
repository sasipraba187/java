package day9;


	interface Animal
	{
		void sound();
		default void run()
		{
			System.out.println("Running");
		}
		static void eat()
		{
			System.out.println("Eating");
		}
	}
class dog implements Animal
{
	public void sound()
	{
		System.out.println("Bow");
	}

	public void display() {
		
		
	}
}
public class problem2 {


	public static void main(String[] args) {
		dog obj=new dog();
		obj.sound();
	}
}
	

