import java.util.Scanner;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        sc.close();

        int trailingZeroes = 0;
        int divisor = 5;

        while (divisor <= N) {
            trailingZeroes += N / divisor;
            divisor *= 5;
        }

        System.out.println(trailingZeroes);
    }
}
